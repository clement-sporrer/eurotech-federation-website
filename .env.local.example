# Google Sheets API Configuration
GOOGLE_SHEETS_ID=your_google_sheet_id_here
GOOGLE_SHEETS_API_KEY=your_google_api_key_here

# Alternative to API key: Service account credentials as JSON string
# Copy the entire JSON content from your service account key file and assign it to this variable
GOOGLE_SERVICE_ACCOUNT_CREDENTIALS={"type":"service_account","project_id":"your-project-id","private_key_id":"...","private_key":"...","client_email":"...","client_id":"...","auth_uri":"...","token_uri":"...","auth_provider_x509_cert_url":"...","client_x509_cert_url":"..."}

# Other environment variables
SENDGRID_API_KEY=your_sendgrid_api_key
SENDGRID_SENDER=your_sender_email
SENDGRID_RECIPIENT=your_recipient_email 